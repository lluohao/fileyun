<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lluohao.fileyun.dao.FileYunDao">
    <insert id="insert" parameterType="YunFile" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        INSERT INTO yunfile (`id`, `name`, `content`, `createTime`, `download`, `status`, `des`, `auth`) VALUES
        (NULL , #{name}, #{content}, #{createTime}, #{download}, #{status}, #{des}, #{auth})
    </insert>
</mapper>